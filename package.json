{
  "name": "moon",
  "version": "1.0.0",
  "description": "一个宠物用药查询平台",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "egg": "^2.15.1",
    "egg-alinode": "^2.0.1",
    "egg-mysql": "^3.0.0",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^5.2.1",
    "egg-session": "^3.1.0",
    "mysql2": "^2.1.0",
    "simpleflakes": "^2.1.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "cross-env": "^7.0.2",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "egg-redis": "^2.4.0",
    "egg-session-redis": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "sequelize-cli": "^5.5.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start-in-docker": "egg-scripts start --title=egg-server-moon",
    "start": "egg-scripts start --daemon --title=egg-server-moon",
    "stop": "egg-scripts stop --title=egg-server-moon",
    "dev": "egg-bin dev",
    "local": "cross-env EGG_SERVER_ENV=local npm run dev",
    "test-server":"cross-env EGG_SERVER_ENV=test-server npm run dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
